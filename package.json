{
  "name": "comic_master",
  "productName": "Electron",
  "private": true,
  "version": "1.0.0",
  "description": "Vite React Electron boilerplate.",
  "author": "KATO_Yuumin",
  "license": "MIT",
  "main": "dist/electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.6",
    "@types/node": "^18.7.8",
    "@types/prettier": "^2.7.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-react": "^2.0.1",
    "autoprefixer": "^10.4.8",
    "electron": "^20.0.3",
    "electron-builder": "^23.3.3",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "sass": "^1.54.5",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4",
    "vite": "^3.0.9",
    "vite-plugin-electron": "^0.9.0"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 7777
  },
  "dependencies": {
    "fflate": "^0.7.3",
    "path-browserify": "^1.0.1",
    "rambda": "^7.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rooks": "^6.4.3",
    "ts-pattern": "^4.0.5",
    "vitest": "^0.22.1",
    "zustand": "^4.1.0"
  },
  "build": {
    "appId": "com.electron.comic_master",
    "productName": "comic_master",
    "directories": {
      "output": "dist/electron"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "mac": {
      "target": "dmg",
      "publish": "github"
    },
    "win": {
      "target": "nsis",
      "publish": "github"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "publish": {
      "provider": "github"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/araera111/comic_master"
  }
}
